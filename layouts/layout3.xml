<?xml version="1.0" encoding="utf-8"?>

<layout type="lmr">

    <groups>
        <group
				class="col-md-3 col-sm-12 col-xs-12"
				noright-class=" col-md-3 col-sm-12 col-xs-12 col-md-pull-9">left</group>
        <group class="col-md-3 col-xs-12" >right</group>
        <group
               class="col-md-6 col-xs-12"
               noright-class="col-md-9 col-xs-12 col-md-push-3"
               noleft-class="col-md-9  col-xs-12"
               noleftright-class=" col-xs-12">main</group>
    </groups>
    <blocks>

        <headertop html5tag="section" order="0" id="yt_header_top">
            <positions>
               <position class="col-md-5  hidden-sm hidden-xs" type="modules" style="ytmod">content_top1</position>
               <position class="col-md-7 col-xs-12" type="modules" style="ytmod">content_top2</position>
            </positions>
        </headertop>
		<headercenter html5tag="section" order="1" id="yt_header_center">
            <positions>
               <position class="col-lg-3 col-md-12 col-xs-12" type="feature" overlogo="logo2">@logo</position>
			   <position class="col-lg-6  col-md-9 col-sm-12 col-xs-12" type="modules" style="ytmod">search</position>
			   <position class="col-lg-3 col-md-3 hidden-sm hidden-xs" type="modules" style="ytmod">content_top4</position>
            </positions>
        </headercenter>
        <headerbottom html5tag="section" order="2" id="yt_header_bottom">
            <positions>
			   <position class="col-lg-3 col-md-3 col-sm-4 col-xs-6" type="modules" style="ytmod">content_top3</position>
			   <position class="col-lg-7 col-md-8 col-sm-4 col-xs-4" type="feature">@mainmenu</position>
			   <position class="col-lg-2 col-md-1 col-sm-4 col-xs-2" type="modules" style="ytmod">content_top5</position>
            </positions>
        </headerbottom>
        <spotlight1 html5tag="section" order="3" id="yt_spotlight1">
            <positions>
                <position class="col-xs-12" type="modules"  style="ytmod" >content_main1</position>
            </positions>
        </spotlight1>
        <breadcrumb html5tag="section" order="4" id="yt_breadcrumb">
            <positions>
                <position class="col-sm-12" type="modules"  style="ytmod" >breadcrumb</position>
            </positions>
        </breadcrumb>
        <content html5tag="section" order="5" id="content">
            <positions>
                
                <position group="main"  type="message"></position>
                <position group="main" type="modules" style="ytmod">main-top</position>
                <position group="main"  type="component">component</position>
                <position group="main" type="modules" style="ytmod">main-bottom</position>
                <position group="left"  type="modules"  style="ytmod" >left</position>
                <position group="right" type="modules"  style="ytmod" >right</position>
            </positions>
        </content>
        <spotlight2 html5tag="section" order="6" id="yt_spotlight2">
            <positions>
                <position class="col-sm-12" type="modules"  style="ytmod" >content_main2</position>
				<position class="col-md-9" type="modules"  style="ytmod" >content_main3</position>
				<position class="col-md-3" type="modules"  style="ytmod" >content_main4</position>
            </positions>
        </spotlight2>
        
        <spotlight3 html5tag="section" order="7" id="yt_spotlight3">
            <positions>
				<position class="col-md-6 col-sm-12" type="modules"  style="ytmod" >content_main6</position>
				<position class="col-md-6 col-sm-12" type="modules"  style="ytmod" >content_main7</position>
            </positions>
        </spotlight3>
        
        <spotlight4 html5tag="section" order="8" id="yt_spotlight4">
            <positions>
               <position class="col-sm-12" type="modules"  style="ytmod" >content_main8</position>
            </positions>
        </spotlight4>
		 <bottom html5tag="section" order="9" id="yt_bottom">
            <positions>
				<position class="col-md-8" type="modules"  style="ytmod" >content_main9</position>
				<position class="col-md-4" type="modules"  style="ytmod" >content_main10</position>
            </positions>
        </bottom>
      
        <yt_spotlight5 html5tag="section" order="10" id="yt_spotlight5">
            <positions>
                <position class="col-xs-15" type="modules"  style="ytmod" >bottom1</position>
                <position class="col-xs-15" type="modules"  style="ytmod" >bottom2</position>
                <position class="col-xs-15" type="modules"  style="ytmod" >bottom3</position>
				<position class="col-xs-15" type="modules"  style="ytmod" >bottom4</position>
				<position class="col-xs-15" type="modules"  style="ytmod" >bottom5</position>
				<position class="col-xs-12" type="modules"  style="ytmod" >bottom6</position>
            </positions>
        </yt_spotlight5>
        <footer html5tag="footer" order="11" id="yt_footer">	
            <positions>
				<position class=" col-xs-12" type="feature">@copyright</position>
				<position class=" col-xs-12" type="modules">footer</position>
            </positions>
        </footer>
    </blocks>
</layout>
